{
  "name": "@cobuild/review-gpt",
  "version": "0.1.0",
  "description": "Shared ChatGPT browser review launcher for Cobuild repos",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cobuildwithus/review-gpt.git"
  },
  "bugs": {
    "url": "https://github.com/cobuildwithus/review-gpt/issues"
  },
  "homepage": "https://github.com/cobuildwithus/review-gpt#readme",
  "scripts": {
    "test": "node --test",
    "changelog:update": "scripts/update-changelog.sh",
    "release:check": "npm test && bash -n src/review-gpt.sh scripts/release.sh scripts/update-changelog.sh scripts/extract-changelog-section.sh && npm pack --dry-run",
    "release:dry-run": "scripts/release.sh patch --dry-run",
    "release:patch": "scripts/release.sh patch",
    "release:minor": "scripts/release.sh minor",
    "release:major": "scripts/release.sh major"
  },
  "bin": {
    "cobuild-review-gpt": "bin/cobuild-review-gpt"
  },
  "files": [
    "bin",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531"
